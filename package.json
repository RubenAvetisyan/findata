{
  "name": "boa-statement-parser",
  "version": "1.4.1",
  "description": "Production-ready Bank of America statement PDF parser with transaction categorization",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "import": "./dist/layout/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js"
    },
    "./output": {
      "types": "./dist/output/index.d.ts",
      "import": "./dist/output/index.js"
    },
    "./plaid": {
      "types": "./dist/plaid/index.d.ts",
      "import": "./dist/plaid/index.js"
    },
    "./supabase": {
      "types": "./dist/supabase/index.d.ts",
      "import": "./dist/supabase/index.js"
    },
    "./categorization": {
      "types": "./dist/categorization/index.d.ts",
      "import": "./dist/categorization/index.js"
    }
  },
  "bin": {
    "parse-boa": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "models",
    "schemas",
    ".env.example",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "parse-boa": "node --loader ts-node/esm src/cli/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bank-of-america",
    "pdf-parser",
    "statement-parser",
    "transaction-categorization",
    "financial"
  ],
  "author": {
    "name": "Ruben Avetisyan",
    "url": "https://github.com/RubenAvetisyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RubenAvetisyan/boa-statement-parser.git"
  },
  "bugs": {
    "url": "https://github.com/RubenAvetisyan/boa-statement-parser/issues"
  },
  "homepage": "https://github.com/RubenAvetisyan/boa-statement-parser#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.94.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.530",
    "pg": "^8.18.0",
    "plaid": "^41.1.0",
    "selfsigned": "^5.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@findata/types": "workspace:*",
    "@findata/pdf-extract": "workspace:*",
    "@findata/categorizer": "workspace:*",
    "@findata/categorizer-ml": "workspace:*",
    "@findata/boa-parser": "workspace:*",
    "@findata/output": "workspace:*",
    "@findata/plaid-bridge": "workspace:*",
    "@findata/store": "workspace:*",
    "@types/node": "^20.14.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
